                <li>
                  <loc>
                    <x>0</x>
                    <y>0</y>
                  </loc>
                </li>
                <li>
                  <loc>
                    <x>28.00688</x>
                    <y>0.7644969</y>
                  </loc>
                </li>
                <li>
                  <loc>
                    <x>52.18179</x>
                    <y>0.9989628</y>
                  </loc>
                </li>
              </points>
            </curve>
          </li>
        </paramMappings>
        <filters>
          <li Class="SoundFilterReverb">
            <baseSetup>
              <dryLevel>0</dryLevel>
              <room>0</room>
              <roomHF>0</roomHF>
              <roomLF>0</roomLF>
              <decayTime>1</decayTime>
              <decayHFRatio>0.5</decayHFRatio>
